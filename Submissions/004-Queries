-- Sql Queries

-- 1 : Designers whose portfolio has preorders >= 1:
SELECT DISTINCT D.*
FROM Designer D
JOIN Portfolio P ON D.portfolioID = P.portfolioID
WHERE P.Preorders >= 1;

-- 2 :  Customers who have liked a product:
SELECT DISTINCT C.*
FROM Customer C
JOIN Likes L ON C.custName = L.custName
WHERE L.ProductID IN (
    SELECT ProductID FROM Product
);

